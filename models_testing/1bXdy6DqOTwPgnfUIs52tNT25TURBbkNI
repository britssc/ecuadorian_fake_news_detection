ero_grad()\n","        outputs = model(input_ids, attention_mask)\n","        loss = criterion(outputs, labels)\n","        loss.backward()\n","        optimizer.step()\n","        total_loss += loss.item()\n","    return total_loss / len(dataloader)\n","\n","def evaluate(model, dataloader):\n","    model.eval()\n","    predictions, true_labels = [], []\n","    with torch.no_grad():\n","        for batch in dataloader:\n","            input_ids, attention_mask, labels = [b.to(device) for b in batch]\n","            outputs = model(input_ids, attention_mask)\n","            preds = torch.argmax(outputs, dim=1).cpu().numpy()\n","            predictions.extend(preds)\n","            true_labels.extend(labels.cpu().numpy())\n","    return predictions, true_labels"],"metadata":{"id":"yMEKp12nykY0","executionInfo":{"status":"ok","timestamp":1746819541346,"user_tz":300,"elapsed":4,"user":{"displayName":"MILAGROS ANAHI DELGADO CAPELO","userId":"00434695135941301153"}}},"execution_count":12,"outputs":[]},{"cell_type":"code","source":["# Entrenamiento\n","for epoch in range(2):\n","    loss = train_epoch(model, train_loader)\n","    print(f\"Epoch {epoch+1} | Train Loss: {loss:.4f}\")"],"metadata":{"colab":{"base_uri":"https://localhost:8080/"},"id":"hKFXpRDuynCS","executionInfo":{"status":"ok","timestamp":1746819552272,"user_tz":300,"elapsed":10922,"user":{"displayName":"MILAGROS ANAHI DELGADO CAPELO","userId":"00434695135941301153"}},"outputId":"c9bcb4df-d5fe-461d-8112-f1201e062323"},"execution_count":13,"outputs":[{"output_type":"stream","name":"stdout","text":["Epoch 1 | Train Loss: 0.6069\n","Epoch 2 | Train Loss: 0.3254\n"]}]},{"cell_type":"code","source":["# Evaluaci√≥n final en test\n","preds, trues = evaluate(model, test_loader)\n","print(classification_report(trues, preds, digits=4))"],"metadata":{"id":"a1Ghp-m0y5wr","colab":{"base_uri":"https://localhost:8080/"},"executionInfo":{"status":"ok","timestamp":1746819552595,"user_tz":300,"elapsed":319,"user":{"displayNam